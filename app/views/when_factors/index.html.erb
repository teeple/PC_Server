<h1>Listing when_factors</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Subfctr name</th>
    <th>Description</th>
    <th>Name</th>
    <th>Value</th>
    <th>Operation</th>
    <th>Target</th>
    <th>Default</th>
    <th>Serivce Type</th>
  </tr>

<% @when_factors.each do |when_factor| %>
  <tr>
    <td><%=h when_factor.id %></td>
    <td><%=h when_factor.subfctr_name %></td>
    <td><%=h when_factor.description %></td>
    <td><%=h when_factor.name %></td>
    <td><%=h when_factor.value %></td>
    <td><%=h when_factor.operation %></td>
    <td><%=h when_factor.target %></td>
    <td><%=h when_factor.default %></td>
    <td><%=h when_factor.service_type %></td>
    <td><%= link_to 'Show', when_factor %></td>
    <td><%= link_to 'Edit', edit_when_factor_path(when_factor) %></td>
    <td><%= link_to 'Destroy', when_factor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New when_factor', new_when_factor_path %>
