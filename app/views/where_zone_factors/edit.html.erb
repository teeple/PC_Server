<h1>Editing where_zone_factor</h1>

<% form_for(@where_zone_factor) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :lat %><br />
    <%= f.text_field :lat %>
  </p>
  <p>
    <%= f.label :lng %><br />
    <%= f.text_field :lng %>
  </p>
  <p>
    <%= f.label :where_zone_id %><br />
    <%= f.text_field :where_zone_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

