<h1>Editing when_day_factor</h1>

<% form_for(@when_day_factor) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :day %><br />
    <%= f.text_field :day %>
  </p>
  <p>
    <%= f.label :day_type %><br />
    <%= f.text_field :day_type %>
  </p>
  <p>
    <%= f.label :when_day_id %><br />
    <%= f.text_field :when_day_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @when_day_factor %> |
<%= link_to 'Back', when_day_factors_path %>