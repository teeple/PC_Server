<h1>Listing who_custom_factors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Operation</th>
    <th>Who custom</th>
  </tr>

<% @who_custom_factors.each do |who_custom_factor| %>
  <tr>
    <td><%=h who_custom_factor.name %></td>
    <td><%=h who_custom_factor.value %></td>
    <td><%=h who_custom_factor.operation %></td>
    <td><%=h who_custom_factor.who_custom_id %></td>
    <td><%= link_to 'Show', who_custom_factor %></td>
    <td><%= link_to 'Edit', edit_who_custom_factor_path(who_custom_factor) %></td>
    <td><%= link_to 'Destroy', who_custom_factor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New who_custom_factor', new_who_custom_factor_path %>