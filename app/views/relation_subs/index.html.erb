<h1>Listing relation_subs</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Parent</th>
    <th>Prd attribute</th>
    <th>Relation</th>
  </tr>

<% @relation_subs.each do |relation_sub| %>
  <tr>
    <td><%=h relation_sub.id %></td>
    <td><%=h relation_sub.parent_id %></td>
    <td><%=h relation_sub.prd_attribute_id %></td>
    <td><%=h relation_sub.relation_id %></td>
    <td><%= link_to 'Show', relation_sub %></td>
    <td><%= link_to 'Edit', edit_relation_sub_path(relation_sub) %></td>
    <td><%= link_to 'Destroy', relation_sub, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New relation_sub', new_relation_sub_path %>
