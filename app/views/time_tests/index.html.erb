<h1>Listing time_tests</h1>

<table>
  <tr>
    <th>Time test</th>
  </tr>

<% @time_tests.each do |time_test| %>
  <tr>
    <td><%=h time_test.time_test %></td>
    <td><%= link_to 'Show', time_test %></td>
    <td><%= link_to 'Edit', edit_time_test_path(time_test) %></td>
    <td><%= link_to 'Destroy', time_test, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New time_test', new_time_test_path %>