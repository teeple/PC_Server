<h1>Editing additional_service</h1>

<% form_for(@additional_service) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :vas_type %><br />
    <%= f.text_field :vas_type %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @additional_service %> |
<%= link_to 'Back', additional_services_path %>
