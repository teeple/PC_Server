<h1>Listing where_custom_factors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Operation</th>
    <th>Where custom</th>
  </tr>

<% @where_custom_factors.each do |where_custom_factor| %>
  <tr>
    <td><%=h where_custom_factor.name %></td>
    <td><%=h where_custom_factor.value %></td>
    <td><%=h where_custom_factor.operation %></td>
    <td><%=h where_custom_factor.where_custom_id %></td>
    <td><%= link_to 'Show', where_custom_factor %></td>
    <td><%= link_to 'Edit', edit_where_custom_factor_path(where_custom_factor) %></td>
    <td><%= link_to 'Destroy', where_custom_factor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New where_custom_factor', new_where_custom_factor_path %>