<h1>New promotion_factor</h1>

<% form_for(@promotion_factor) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :promotion_id %><br />
    <%= f.text_field :promotion_id %>
  </p>
  <p>
    <%= f.label :cond_or_result %><br />
    <%= f.text_field :cond_or_result %>
  </p>
  <p>
    <%= f.label :is_required %><br />
    <%= f.text_field :is_required %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', promotion_factors_path %>
