<h1>Listing what_customs</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Subfctr name</th>
    <th>Description</th>
  </tr>

<% @what_customs.each do |what_custom| %>
  <tr>
    <td><%=h what_custom.id %></td>
    <td><%=h what_custom.subfctr_name %></td>
    <td><%=h what_custom.description %></td>
    <td><%= link_to 'Show', what_custom %></td>
    <td><%= link_to 'Edit', edit_what_custom_path(what_custom) %></td>
    <td><%= link_to 'Destroy', what_custom, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New what_custom', new_what_custom_path %>
