<h1>Listing what_custom_factors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Operation</th>
    <th>What custom</th>
  </tr>

<% @what_custom_factors.each do |what_custom_factor| %>
  <tr>
    <td><%=h what_custom_factor.name %></td>
    <td><%=h what_custom_factor.value %></td>
    <td><%=h what_custom_factor.operation %></td>
    <td><%=h what_custom_factor.what_custom_id %></td>
    <td><%= link_to 'Show', what_custom_factor %></td>
    <td><%= link_to 'Edit', edit_what_custom_factor_path(what_custom_factor) %></td>
    <td><%= link_to 'Destroy', what_custom_factor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New what_custom_factor', new_what_custom_factor_path %>